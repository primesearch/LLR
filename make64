# Compile the LLR source files for X86_64 architecture

D = ./linux64llr

CC = gcc
CFLAGS = -I. -I./gwnum -DX86_64 -O2

CPP = g++
CPPFLAGS = -I. -I./gwnum -DX86_64 -O2

COMPOBJS = lprime.o lmenu.o

compile: $(COMPOBJS)
	mv $(COMPOBJS) $(D)

lprime.o: lprime.c
	$(CC) $(CFLAGS) -c lprime.c

lmenu.o: lmenu.c
	$(CC) $(CFLAGS) -c lmenu.c

.cpp.o:
	$(CPP) $(CPPFLAGS) -c $<

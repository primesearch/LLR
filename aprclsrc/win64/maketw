# Compile the TW source files for Windows 64 bits architecture (cygwin)
# Please, adjust options for 64 bits!

D = ../

CC = gcc
CFLAGS = -I../ -O2
sources = $(D)/tw.c $(D)/mpz_aprcl.c
LIBS = libgmp.a
EXE = tw.exe

$(EXE): $(sources)
	$(CC) $(CFLAGS) -o $(EXE) $(sources) $(LIBS)

clean:
	rm $(EXE)
